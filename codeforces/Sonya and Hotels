#include<bits/stdc++.h>
using namespace std;
int main(){
    int n=0;
    int d=0, j=0;
    set<int>s;
    cin>>n>>d;
    int k=2*n;
    int ar[n];
    int coor[k];
    
    for(int i=0;i<n;i++){
    cin>>ar[i];
    coor[j]=ar[i]+d;
    j++;
    coor[j]=ar[i]-d;
    j++;
    }
    
    for(int i=0;i<k;i++){
        int temp=coor[i];
        cout<<temp<<" ";
        for(int j=0;j<n;j++){
            int n=ar[j];
            if(abs(n-temp)>=d)
            s.insert(temp);
        }
    }
}
